def rev_arr(a): #using while loop 
	i = 0
	j = len(a)-1
	while i < j:
		temp = a[i]
		a[i] = a[j]
		a[j] = temp
		i += 1
		j -= 1
	return a
	
a = [2, 3, 4, 56, 23]
print(rev_arr(a))
o/p : [23, 56, 4, 3, 2]

def rev_arr(a):
    for i in range(len(a)//2):
        a[i], a[len(a)-1-i] = a[len(a)-1-i], a[i]
    return a

a = [2, 3, 4, 56, 23]
print(rev_arr(a))   

o/p : [23, 56, 4, 3, 2]
    
def arr(a):
	return reversed(a)
a = [2, 3, 4, 56, 23]
print(rev_arr(a))   

o/p : [23, 56, 4, 3, 2]

def rev_arr(a):
    return list(reversed(a)) 
	
a = [2, 3, 4, 56, 23]
print(rev_arr(a))   

o/p : [23, 56, 4, 3, 2]

def rev_arr(a):
	return a[::-1]

a = [2, 3, 4, 56, 23]
print(rev_arr(a))   

o/p : [23, 56, 4, 3, 2] 
